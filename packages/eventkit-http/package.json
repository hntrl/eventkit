{
  "name": "@eventkit/http",
  "version": "0.2.0",
  "license": "MIT",
  "author": "Hunter Lovell <hunter@hntrl.io>",
  "description": "HTTP helpers for eventkit",
  "repository": {
    "type": "git",
    "url": "https://github.com/hntrl/eventkit",
    "directory": "packages/eventkit-http"
  },
  "keywords": [
    "eventkit",
    "http",
    "fetch",
    "sse",
    "eventsource",
    "observable",
    "streaming"
  ],
  "files": [
    "dist/",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "wireit",
    "typecheck": "tsc"
  },
  "wireit": {
    "build": {
      "command": "tsup",
      "files": [
        "lib/**",
        "tsup.config.ts",
        "tsconfig.json",
        "package.json"
      ],
      "output": [
        "dist/**"
      ]
    }
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "@eventkit/base": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@types/ws": "^8.18.1",
    "eventsource": "^3.0.6",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "undici-types": "^7.5.0",
    "wireit": "^0.14.11",
    "ws": "^8.18.1"
  }
}
